<div class="container">
    <div class = "row"> 
        <div class ="col-3 mt-4">
            <%= render 'indexleftbar' %>
        </div>

        <div class = "col-9">
            <div class = "card mt-4">
                <div class = "card-header">
                    Your Listings 
                </div>    

                <div class="card-body">
                    <div class = "container">
                        <div class = "row">
                        <ul>
                                <% current_user.properties.each do |f| %>
                                <li>
                                    room.photo | listing_name | <%= link_to 'Update', listing_property_path(f), class: "btn btn-info" %>
                                </li>
                                <% end%>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>